{
  "name": "@fjedi/rest-api",
  "version": "2.2.4",
  "description": "All-in-one REST API server",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "private": false,
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watchAll",
    "build": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "clean": "rimraf lib",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/rest-api.git"
  },
  "keywords": [
    "fjedi",
    "rest-api"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/rest-api/issues"
  },
  "homepage": "https://github.com/fjedi/rest-api#readme",
  "dependencies": {
    "@fjedi/database-client": "4.2.2",
    "@fjedi/errors": "1.1.4",
    "@fjedi/jwt": "1.0.4",
    "@fjedi/logger": "1.46.3",
    "@fjedi/redis-client": "1.157.0",
    "@sentry/integrations": "6.12.0",
    "@sentry/node": "6.12.0",
    "@types/component-emitter": "1.2.10",
    "@types/cookie": "0.4.1",
    "@types/cors": "2.8.12",
    "@types/geoip-lite": "1.4.1",
    "@types/git-rev-sync": "2.0.0",
    "@types/http-terminator": "2.0.2",
    "@types/invariant": "2.2.35",
    "@types/ioredis": "4.27.1",
    "@types/ip": "1.1.0",
    "@types/json-buffer": "3.0.0",
    "@types/kcors": "2.2.4",
    "@types/koa": "2.13.4",
    "@types/koa-bodyparser": "4.3.3",
    "@types/koa-helmet": "6.0.4",
    "@types/koa-passport": "4.0.2",
    "@types/koa-router": "7.4.4",
    "@types/socket.io-emitter": "3.1.0",
    "@types/useragent": "2.3.1",
    "@types/uuid": "8.3.1",
    "@types/yup": "0.29.13",
    "bignumber.js": "9.0.1",
    "bluebird": "3.7.2",
    "boxen": "5.0.1",
    "chalk": "4.1.2",
    "cookie": "0.4.1",
    "cookies": "0.8.0",
    "cors": "2.8.5",
    "dayjs": "1.10.6",
    "eiows": "3.6.1",
    "geoip-lite": "1.4.2",
    "git-rev-sync": "3.0.1",
    "http": "0.0.1-security",
    "http-terminator": "3.0.0",
    "i18next": "20.6.0",
    "i18next-sync-fs-backend": "1.1.1",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "json-buffer": "3.0.1",
    "kcors": "2.2.2",
    "koa": "2.13.1",
    "koa-bodyparser": "4.3.0",
    "koa-helmet": "6.1.0",
