    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.5",
    "typescript": "4.4.2"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "npm run lint:fix",
      "git add"
    ]
  }
