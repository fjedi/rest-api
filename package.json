    "@fjedi/jwt": "1.0.3",
    "@fjedi/logger": "1.46.1",
    "@fjedi/redis-client": "1.0.2",
    "@sentry/integrations": "5.30.0",
    "@sentry/node": "5.30.0",
    "@types/component-emitter": "1.2.10",
    "@types/cookie": "0.4.0",
    "@types/cors": "2.8.10",
    "@types/geoip-lite": "1.4.1",
    "@types/git-rev-sync": "2.0.0",
    "@types/http-terminator": "2.0.1",
    "@types/invariant": "2.2.34",
    "@types/ioredis": "4.22.2",
    "@types/ip": "1.1.0",
    "@types/json-buffer": "3.0.0",
